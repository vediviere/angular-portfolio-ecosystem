{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "ng serve",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/node_modules/@angular/cli/bin/ng",
      "args": ["serve"],
      "console": "integratedTerminal"
    },
    {
      "name": "ng test",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/node_modules/.bin/jest",
      "args": ["--watch"],
      "console": "integratedTerminal"
    }
  ]
}
